* {margin:0;padding:0;}

body {
	font-size: 14px;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-box-align: stretch;
	background-color: #efefef;
}

input[type="phone"],input[type="text"],input[type="password"],input[type="number"]{
    padding:0 5px;
}

input[type="search"] {
      -webkit-appearance: textfield;
}

input[type="search"]::-webkit-search-cancel-button {
	-webkit-appearance: none;
}

img {border:0;}
ul {list-style:none;}
i {font-style:normal;}
a {color:inherit;text-decoration:none;-webkit-border-radius:5px;}
hr{ border: none; border-top: 1px solid #ddd; margin: 15px 0;transition: border-top-color 0.2s linear 0s, width 0.3s linear 0s; }

